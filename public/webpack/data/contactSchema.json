{
  "contactSchema":{
        "type": "object",
        "properties": {
          "first name": {
            "type": "string",
            "description": "First Name",
            "minLength": 2
          },
          "last name": {
            "type": "string",
            "description": "Last Name",
            "minLength": 2
          },
          "email": {
            "type": "string",
            "description": "Email",
            "minLength": 5
          },
          "phone": {
            "type": "string",
            "description": "Phone",
            "minLength": 7
          },
          "address": {
            "type": "object",
            "properties": {
                  "street": {
                    "type": "string",
                    "description": "Street",
                    "minLength": 3
                  },
                  "city": {
                    "type": "string",
                    "description": "City",
                    "enum": [
                        "St. Louis",
                      ],
                    "minLength": 3
                  },
                  "state": {
                    "type": "string",
                    "description": "State",
                    "enum": [
                      "MO",
                      "IL"
                    ],
                    "minLength": 2
                  },
                  "zipcode": {
                    "type": "string",
                    "description": "Zipcode",
                    "minLength": 6
                  }
              }
          },
          "businessAddress": {
              "type": "object",
              "properties": {
                    "street": {
                      "type": "string",
                      "description": "Street",
                      "minLength": 3
                    },
                    "city": {
                      "type": "string",
                      "description": "City",
                      "minLength": 3
                    },
                    "state": {
                      "type": "string",
                      "description": "State",
                      "minLength": 2
                    },
                    "zipcode": {
                      "type": "string",
                      "description": "Zipcode",
                      "minLength": 6
                    }
              }
        }
    }

}
